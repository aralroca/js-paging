{
  "name": "js-paging",
  "version": "0.2.0",
  "description": "Paging badges generator",
  "source": "index.js",
  "main": "out/index.js",
  "types": "index.d.ts",
  "scripts": {
    "build": "bun build --minify --outdir=out index.ts",
